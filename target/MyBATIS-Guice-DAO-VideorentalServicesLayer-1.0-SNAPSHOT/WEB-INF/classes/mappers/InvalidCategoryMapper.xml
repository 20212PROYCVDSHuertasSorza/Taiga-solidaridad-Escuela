<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.eci.cvds.sampleprj.dao.mybatis.mappers.InvalidCategoryMapper">

    <select id="searchInvalidCategory" parameterType="map" resultMap="InvalidCategoryResult">
        select * from InvalidCategory;
    </select>

    <select id="searchInvalidCategory" parameterType="map" resultMap="InvalidCategoryResult">
        select * from InvalidCategory where word=#{word};
    </select>

    <resultMap id="InvalidCategoryResult" type="InvalidCategory">
        <id property="idic" column="idic"/>
        <result property="word" column="word"/>
    </resultMap>
</mapper>